# Cloudflare R2 Storage Configuration
# Get these from Cloudflare Dashboard: https://dash.cloudflare.com
R2_ENDPOINT=https://your-account-id.r2.cloudflarestorage.com
R2_ACCESS_KEY_ID=your_access_key_here
R2_SECRET_ACCESS_KEY=your_secret_key_here
R2_BUCKET=miniapps
R2_BUCKET_STAGING=miniapps-staging
R2_BUCKET_PRODUCTION=miniapps

# Cloudflare API Configuration
# Create API Token with R2 read/write permissions
CF_API_TOKEN=your_api_token_here
CF_ACCOUNT_ID=your_account_id_here

# Supabase Configuration (if using)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
SUPABASE_ACCESS_TOKEN=your_access_token_here

# Neo N3 Blockchain Configuration
NEO_RPC_URL=https://testnet1.neo.coz.io:443
NEO_MAINNET_RPC_URL=https://mainnet1.neo.coz.io:443

# Deployment Configuration
DEPLOY_ENVIRONMENT=development
