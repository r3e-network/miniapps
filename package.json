{
  "name": "meshminiapp-uniapp",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.27.0",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "build:all": "node scripts/build/all.js",
    "build:analyze": "node scripts/build/all.js --analyze",
    "generate": "node scripts/generate-templates.js",
    "test": "turbo test",
    "test:run": "turbo test:run",
    "test:coverage": "vitest run --coverage",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean && rm -rf node_modules/.cache public/miniapps",
    "format": "prettier --write \"**/*.{ts,tsx,vue,json,md}\"",
    "verify:env": "node scripts/verify-env.js",
    "r2:ls": "node scripts/list-buckets.js",
    "r2:upload": "node scripts/deploy/upload-built.js production",
    "deploy": "node scripts/deploy/to-r2.js",
    "deploy:dev": "DEPLOY_ENVIRONMENT=development node scripts/deploy/to-r2.js",
    "deploy:staging": "DEPLOY_ENVIRONMENT=staging node scripts/deploy/to-r2.js",
    "deploy:prod": "DEPLOY_ENVIRONMENT=production node scripts/deploy/to-r2.js",
    "registry:generate": "node scripts/auto-discover-miniapps.js",
    "registry:sync": "node scripts/deploy/sync-registry.js",
    "publish": "node scripts/deploy/publish.js",
    "publish:all": "node scripts/deploy/publish.js",
    "register": "node scripts/deploy/register.js",
    "register:all": "node scripts/deploy/register.js",
    "supabase:check": "node scripts/check-supabase-duplicates.js",
    "supabase:cleanup": "node scripts/cleanup-supabase-duplicates.js",
    "supabase:cleanup:execute": "node scripts/cleanup-supabase-duplicates.js --execute",
    "supabase:list": "node scripts/list-supabase-tables.js",
    "register:full": "node scripts/deploy/to-supabase-upsert.js",
    "register:verify": "node scripts/verify-registration.js",
    "register:clean": "node scripts/clean-all-miniapps.js"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.709.0",
    "@dcloudio/types": "^3.4.28",
    "@vitejs/plugin-vue": "^5.1.0",
    "@vitest/coverage-v8": "^2.1.0",
    "@vue/test-utils": "^2.4.6",
    "dotenv": "^16.4.7",
    "jsdom": "^25.0.0",
    "miniprogram-api-typings": "^4.1.2",
    "terser": "^5.46.0",
    "turbo": "^2.7.5",
    "typescript": "^5.4.5",
    "vitest": "^2.1.0",
    "vue": "^3.5.0"
  },
  "pnpm": {
    "overrides": {
      "ws": ">=7.5.10",
      "elliptic": ">=6.5.7",
      "lodash": ">=4.17.21",
      "@intlify/core-base": ">=9.14.0",
      "@intlify/message-resolver": ">=9.1.11",
      "@intlify/shared": ">=9.14.0",
      "qs": ">=6.13.0",
      "esbuild": ">=0.25.0",
      "jspdf": ">=4.0.0"
    }
  }
}
